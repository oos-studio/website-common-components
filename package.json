{
  "name": "@oos-studio/website-common-components",
  "version": "0.0.7-alpha",
  "description": "A directory of commonly used components for building websites with react",
  "main": "src/components/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "deepmerge": "^4.2.2",
    "prop-types": "^15.7.2",
    "react-parallax": "^3.0.3",
    "react-render-html": "^0.6.0",
    "react-scroll-parallax": "^2.3.1",
    "reactstrap": "^8.4.1"
